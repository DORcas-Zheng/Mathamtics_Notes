    "## Addition\n",
    "print(\"Vector addiction: \", v + w)\n",
    "## Scaling\n",
    "print(\"Vector Scaling: \", 2 * v)\n",
    "## Dot\n",
    "print(\"Vector dot: \", np.dot(v, w))\n",
    "\n"
